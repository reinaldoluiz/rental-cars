<h1>Modelos de carro</h1>

<%= link_to 'Voltar', root_path %>

<%= link_to 'Registrar um modelo de carro', new_car_model_path %>

<% if @car_models.empty? then %>
   Nenhum modelo cadastrado
<%else%>
  <% @car_models.each do |model| %>
    <%= link_to "#{model.name} - #{model.year}", model%>
  <%end%>
<% end %>